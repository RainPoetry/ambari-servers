<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
<!-- Elastic search  Configurations -->

<configuration supports_adding_forbidden="true">
    <!-- Configurations -->
    <property>
        <name>storage_base_path</name>
        <value>/var/data/fdfs/storage</value>
        <description>Cluster name identifies your cluster</description>
    </property>
    <property>
        <name>storage_data_path</name>
        <value>/var/data/fdfs/storage_data</value>
        <description>Cluster name identifies your cluster</description>
    </property>
    <property>
        <name>storage_group_name</name>
        <value>cc</value>
        <description>Cluster name identifies your cluster</description>
    </property>
    <property>
        <name>storage_port</name>
        <value>23000</value>
        <description>Cluster name identifies your cluster</description>
    </property>
    <property>
        <name>storage_http_server_port</name>
        <value>8888</value>
        <description>Cluster name identifies your cluster</description>
    </property>
</configuration>